class Solution {
public:
    int countTriples(int n) {
        int ans = 0;
        for(int a = 1; a<=n; a++){
            for(int b = a + 1; b<=n ; b++){
                int sqr = a*a + b*b;
                int c = (int)sqrt(sqr);
                if(c*c == sqr && c <= n) ans += 2;
            }
        }
        return ans;
    }
};
