class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        vector<int> nums2;
        int n = nums.size();
        for(int i = 0; i < n; i++){
            int x = abs(nums[i]);
            if(nums[x-1]>0)
               nums[x-1] = -nums[x-1];
        }
        for(int i = 0; i < n; i++){
            if(nums[i] > 0) nums2.push_back(i+1);
        }
        return nums2;
    }
};
