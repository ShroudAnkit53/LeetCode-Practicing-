class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        int n = nums.size();
        vector<int> ans;
        unordered_map<int,int> mpp;

        // Count frequency of each element
        for (auto it : nums) {
            mpp[it]++;
        }

        // Check which elements occur more than n/3 times
        for (auto it : mpp) {
            if (it.second > n / 3) {
                ans.push_back(it.first);
            }
        }

        return ans;
    }
};
