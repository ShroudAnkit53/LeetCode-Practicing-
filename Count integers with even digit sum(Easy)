class Solution {
private:
    bool Sum(int n){
        int sum = 0;
        while(n){
            int digit = n % 10;
            sum += digit;
            n /= 10;
        }
        if(sum % 2 == 0) return true;
        else return false;
    }
public:
    int countEven(int num) {
        int count = 0;
        for(int i = 1; i <= num; i++){
            if(Sum(i) == true) count++;
        }
        return count;
    }
};
