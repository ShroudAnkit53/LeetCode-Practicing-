class Solution {
public:
    string largestGoodInteger(string num) {
        string max_good = "";
        for(int i = 0; i <= (int)num.size()-3;i++){
            if(num[i] == num[i+1] && num[i] == num[i+2]){
                string sub = num.substr(i, 3);
                if(sub > max_good) max_good = sub;
            }
        }
        return max_good;
    }
};
