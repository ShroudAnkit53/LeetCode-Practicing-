class Solution {
public:
    int busyStudent(vector<int>& startTime, vector<int>& endTime, int queryTime) {
        int m = startTime.size();
        int count = 0;
        for(int i = 0; i < m; i++){
            if(startTime[i] <= queryTime && queryTime <= endTime[i]) count++;
        }
        return count;
    }
};
