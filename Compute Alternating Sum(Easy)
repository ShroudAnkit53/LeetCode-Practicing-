class Solution {
public:
    int alternatingSum(vector<int>& nums) {
        int alt_sum = 0;
        for(int i = 0; i < nums.size(); i++){
            if(i%2 == 0){
                alt_sum += nums[i];
            }else{
                alt_sum -= nums[i];
            }
        }
        return alt_sum;
    }
};
